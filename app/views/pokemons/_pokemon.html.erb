<li id="pokemon-<%= pokemon.id %>">
  <span class="name"><%= pokemon[:poke_id] %></span> <br>
  Move 1: <span class="move"><%= pokemon[:move_1] %></span> <br>
  Move 2: <span class="move"><%= pokemon[:move_2] %></span> <br>
  Max Health: <span class="health"><%= pokemon[:health] %></span> <br>
  IV Stats = 
  Attack: <span class="stat"><%= pokemon[:attack] %></span>
  Defense: <span class="stat"><%= pokemon[:defense] %></span>
  Stamina: <span class="stat"><%= pokemon[:stamina] %></span>
</li>
